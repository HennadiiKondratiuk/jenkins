{
    "family": "nginx_family", 
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::319448237430:role/ecsTaskExecutionRole", 
    "containerDefinitions": [
        {
            "name": "nginx", 
            "image": "319448237430.dkr.ecr.eu-central-1.amazonaws.com/hkondratiuk-images:$BUILD_ID", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ]
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
